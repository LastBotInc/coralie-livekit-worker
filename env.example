# LiveKit Agent Worker Configuration
# Copy this file to .env and fill in your values

# Required: LiveKit server connection
LIVEKIT_URL=ws://localhost:7880
LIVEKIT_API_KEY=devkey
LIVEKIT_API_SECRET=devsecret

# Required: Coralie Conference Service
CONFERENCE_GRPC=localhost:9090
CONFERENCE_WORKER_ID=coralie-livekit-worker-1

# Optional: Agent configuration
# LK_AGENT_NAME=my-agent
# LK_NAMESPACE=default
# LK_JOB_TYPE=JT_ROOM

# Optional: Performance and behavior
# LK_DRAIN_TIMEOUT=30s
# LK_MAX_CONCURRENT_JOBS=8
# LK_LOG_LEVEL=info
# LK_PPROF_ADDR=127.0.0.1:6060
# LK_JOB_TIMEOUT=5m
# AGGRESSIVE_LOGGING=false
